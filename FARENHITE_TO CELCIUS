/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		
		
		System.out.println("Enter Farenhite temparature")
		    
		    float F=sc.nextFloat();
		    double C=0;
		
		        C=(0.555555555)*(F-32);   //*as formula we know that c/5=(f-32)/9;
		
		            System.out.println("In celcius the temperature is "+C);


	}
}
