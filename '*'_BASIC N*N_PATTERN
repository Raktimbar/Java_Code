/* package codechef; // don't place package name! */

import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Codechef
{
	public static void main (String[] args) throws java.lang.Exception
	    {
	       
	       
	       Scanner sc=new Scanner(System.in);
	       
	       int n=sc.nextInt();
	       
	        for(int j=0;j<=n;j++)   //*this loop print the repeatation of same numbes of internal loop rows with columns repeatation
	        {  
	            
	                        for(int i=1;i<=n;i++)  //*This loops creates * with space internally
	                            {
	                                System.out.print("* ");
	                           
	                            }

            System.out.println();
	       
	       
	        }
	        
	       
	       
	     }

    
}
